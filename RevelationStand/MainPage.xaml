<Page
    x:Class="RevelationStand.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:RevelationStand"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    >


    <Grid>
        <!--Разметка левой панели сплит контейнера(Меню) -->
        <SplitView x:Name="MainView" PaneBackground="White" OpenPaneLength="200" DisplayMode="Inline" IsPaneOpen="True">
            <SplitView.Pane>
                <Grid Visibility="Visible">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="3*"/>
                        <RowDefinition Height="10*"/>
                        <RowDefinition Height="10*"/>
                        <RowDefinition Height="10*"/>
                        <RowDefinition Height="10*"/>
                        <RowDefinition Height="10*"/>
                        <RowDefinition Height="10"/>
                        <RowDefinition Height="2*"/>
                    </Grid.RowDefinitions>
                    <TextBlock x:Name="charactetistic" Grid.Row="1" HorizontalAlignment="Center" VerticalAlignment="Center" Text="Menu" FontSize="14" TextAlignment="Center"/>
                    <ListView  x:Name="navButtons"     Grid.Row="2" Grid.RowSpan="4" HorizontalAlignment="Stretch" VerticalAlignment="Top" Margin="10,20,0,0" SelectionChanged="Selected">
                        <ListViewItem x:Name="goToCharacteristicGrid" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="Харакетристика" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                        <ListViewItem x:Name="goToArmorGrid"          HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="Броня"          HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                        <ListViewItem x:Name="goToVehaGrid"           HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="Вехи"           HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                        <ListViewItem x:Name="goToStoneGrid"          HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="Брактеат"       HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                        <ListViewItem x:Name="goToSkillGrid"          HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="Умения"         HorizontalContentAlignment="Center" VerticalContentAlignment="Center" />
                    </ListView>
                    <StackPanel Orientation="Horizontal" Grid.Row="6" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <SymbolIcon Symbol="Setting"/>
                        <TextBlock Text="Settings" Margin="10,0,0,0"/>
                    </StackPanel>
                </Grid>
            </SplitView.Pane>

            <!--Разметка правого контейнера сплит панели-->
            <Grid Background="{StaticResource AppBarBackgroundThemeBrush}">
                <!--Разметка окна характеристик-->
                <Grid x:Name="characteristicGrid" Visibility="Visible">
                    <Grid.Background>
                        <ImageBrush ImageSource="Images/Char.jpg" Stretch="UniformToFill" Opacity="0.5" />
                    </Grid.Background>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="6*"/>
                        <RowDefinition Height="6*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <TextBlock x:Name="characteristicWindow" Grid.Row="0" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" FontSize="14" Text="Характеристики" Margin="30,0,0,0"/>
                    <TextBlock x:Name="lvl"                  Grid.Row="0" Grid.Column="1" HorizontalAlignment="Right" VerticalAlignment="Center" FontSize="14" Text="" Margin="30,0,0,0"/>
                    <Grid Grid.Row="1" Grid.Column="1" Visibility="Visible">
                        <!--Окно характеристик-->
                        <Grid.RowDefinitions>
                            <RowDefinition Height="*"/>
                            <RowDefinition MinHeight="50" Height="10*"/>
                            <RowDefinition MinHeight="50" Height="10*"/>
                            <RowDefinition MinHeight="50" Height="10*"/>
                            <RowDefinition MinHeight="50" Height="10*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition MinWidth="100" Width="10*"/>
                            <ColumnDefinition MinWidth="100" Width="10*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>

                        <StackPanel Grid.Row="2" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Spacing="20" Margin="10,10,0,0">
                            <TextBlock Text="Сила" FontSize="15"  FontFamily="Franklin Gothic Medium"  ToolTipService.ToolTip="Каждая единица силы увеличивает на 1 ед. модификатор силы атаки. Каждые 10 единиц силы увеличивают бонус к базовой силе атаки на 4%"/>
                            <Button x:Name="subStrange" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="60,0,0,0" Padding="0,0,0,0" Content="-" Tapped="sub_click" />
                            <TextBlock Text="{Binding Druid.Strange.Value}" />
                            <Button x:Name="addStrange" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0,0,0,0" Content="+" Tapped="add_click"/>
                            <TextBlock Margin="10,0,0,0" Text="{Binding Druid.Strange.Bonus.Value}" ToolTipService.ToolTip="Бонус к характеристике"/>
                        </StackPanel>
                        <StackPanel Grid.Row="3" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Spacing="20" Margin="10,10,0,0">
                            <TextBlock Text="Интеллект" FontSize="15"  FontFamily="Franklin Gothic Medium" ToolTipService.ToolTip="Каждая единица интеллекта, увеличивает модификатор силы заклинаний на 1 ед., модификатор лечения на 0,6 ед. Каждые 10 единиц, увеличивают бонус к базовой силе заклинаний на 4%"/>
                            <Button x:Name="subIntellegency" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="25,0,0,0" Padding="0,0,0,0" Content="-" Tapped="sub_click"/>
                            <TextBlock Text="{Binding Druid.Intellegency.Value}"/>
                            <Button x:Name="addIntellegency" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0,0,0,0" Content="+" Tapped="add_click"/>
                            <TextBlock Margin="10,0,0,0" Text="{Binding Druid.Intellegency.Bonus.Value}" ToolTipService.ToolTip="Бонус к характеристике"/>
                        </StackPanel>
                        <StackPanel Grid.Row="4" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Spacing="20" Margin="10,10,0,0">
                            <TextBlock Text="Выносливость" FontSize="15"  FontFamily="Franklin Gothic Medium" ToolTipService.ToolTip="Каждая единица выносливости, увеличивает запас здоровья на 29 ед., модификатор(!) защиты на 8ед. Каждые 10 единиц выносливости увеличивают бонус к базовой защите на 4%"/>
                            <Button x:Name="subEndurancy" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="0,0,0,0" Padding="0,0,0,0" Content="-" Tapped="sub_click"/>
                            <TextBlock Text="{Binding Druid.Endurancy.Value}"/>
                            <Button x:Name="addEndurancy" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="0,0,0,0" Padding="0,0,0,0" Content="+" Tapped="add_click"/>
                            <TextBlock Margin="10,0,0,0" Text="{Binding Druid.Endurancy.Bonus.Value}" ToolTipService.ToolTip="Бонус к характеристике"/>
                        </StackPanel>
                        <StackPanel Grid.Row="2" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Spacing="20" Margin="10,10,0,0">
                            <TextBlock Text="Сила духа" FontSize="15"  FontFamily="Franklin Gothic Medium" ToolTipService.ToolTip="Каждая единица силы духа, увеличивает запас здоровья на 25 ед., запас маны на 10 ед., модификатор лечения на 1,6 ед., модификатор сопротивления на 8 ед. Каждые 10 единиц силы духа, увеличивают бонус к базовому сопротивлению на 4%"/>
                            <Button x:Name="subSpellPower" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="0,0,0,0" Padding="0,0,0,0" Content="-" Tapped="sub_click"/>
                            <TextBlock Text="{Binding Druid.SpellPower.Value}"/>
                            <Button x:Name="addSpellPower" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="0,0,0,0" Padding="0,0,0,0" Content="+" Tapped="add_click"/>
                            <TextBlock Margin="10,0,0,0" Text="{Binding Druid.SpellPower.Bonus.Value}" ToolTipService.ToolTip="Бонус к характеристике"/>
                        </StackPanel>
                        <StackPanel Grid.Row="3" Grid.Column="2" HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Spacing="20" Margin="10,10,0,0">
                            <TextBlock Text="Ловкость" FontSize="15"  FontFamily="Franklin Gothic Medium" ToolTipService.ToolTip="Каждая единица ловкости, увеличивает критический удар на 2 ед."/>
                            <Button x:Name="subAgility" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Margin="0,0,0,0" Padding="0,0,0,0" Content="-" Tapped="sub_click"/>
                            <TextBlock Text="{Binding Druid.Agility.Value}"/>
                            <Button x:Name="addAgility" HorizontalContentAlignment="Stretch" VerticalContentAlignment="Stretch" Padding="0,0,0,0" Content="+" Tapped="add_click"/>
                            <TextBlock Margin="10,0,0,0" Text="{Binding Druid.Agility.Bonus.Value}" ToolTipService.ToolTip="Бонус к характеристике"/>
                        </StackPanel>

                        <StackPanel Grid.Row="1" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" Orientation="Horizontal" Spacing="50" Margin="10,10,0,0">
                            <TextBlock Text="Здоровье" FontSize="16"/>
                            <TextBlock Text="{Binding Druid.HP.Value}" FontSize="16"/>
                        </StackPanel>
                        <StackPanel Grid.Row="1" Grid.Column="2" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Center"  Spacing="50" Margin="10,10,0,0">
                            <TextBlock Text="Мана" FontSize="16"/>
                            <TextBlock Text="{Binding Druid.MP.Value}" FontSize="16"/>
                        </StackPanel>
                    </Grid>
                    <!--Все характеристики. 2 Часть SplitView. Блок характеристик -->
                    <ListView Grid.Row="2" Grid.Column="1" Margin="20,20,0,0" Visibility="Visible" Background="#FFECE9E3">
                        <!--Окно характеристик-->
                        <StackPanel Orientation="Horizontal" Spacing="10">
                            <!--Базовая сила атаки + модификатор-->
                            <TextBlock Text="Сила Атаки" />
                            <TextBlock Text="{Binding Druid.Strange.Modifier.Value}" />
                            <TextBlock Text="-" Width="5"/>
                            <TextBlock Text="{Binding Druid.Strange.Modifier.Value}" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Бонус к силе атаки"/>
                            <TextBlock Text="{Binding Druid.Strange.BonusBase.Value}" ToolTipService.ToolTip="Процентное увеличение базовой мин и макс СА"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="10">
                            <TextBlock Text="Сила заклинаний"/>
                            <TextBlock Text="{Binding Druid.Intellegency.Modifier.Value}"/>
                            <TextBlock Text="-" Width="5"/>
                            <TextBlock Text="{Binding Druid.Intellegency.Modifier.Value}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Бонус к силе заклинаний"/>
                            <TextBlock Text="{Binding Druid.Intellegency.BonusBase.Value}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Скорость"/>
                            <TextBlock Text="{Binding Druid.Speed.Value}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Точность"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Критический удар"/>
                            <TextBlock Text="{Binding Druid.CriticalChance.Value}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Критический урон"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Обход защиты"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Игнорирование сопротивления"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <!--ЗАЩИТА (class Defence)-->
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <ToolTipService.ToolTip>
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <TextBlock Text="Базовая защита :"/>
                                    <TextBlock x:Name="baseDefence" Text="{Binding Druid.Defence.Base}"/>
                                    <TextBlock Text="Модификатор :"/>
                                    <TextBlock x:Name="modifierDefence" Text="{Binding Druid.Defence.Modifier.Value}"/>
                                </StackPanel>
                            </ToolTipService.ToolTip>
                            <TextBlock Text="Защита"/>
                            <TextBlock Text="{Binding Druid.Defence.Value}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Бонус к защите"/>
                            <TextBlock Text="{Binding Druid.Endurancy.BonusBase.Value}"/>
                        </StackPanel>
                        <!--СОПРОТИВЛЕНИЕ (class Resist)-->
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <ToolTipService.ToolTip>
                                <StackPanel Orientation="Horizontal" Spacing="5">
                                    <TextBlock Text="Базовое сопротивление:"/>
                                    <TextBlock x:Name="baseResist" Text="{Binding Druid.Resist.Base}"/>
                                    <TextBlock Text=" Модификатор:"/>
                                    <TextBlock x:Name="modifierResist" Text="{Binding Druid.Resist.Modifier.Value}"/>
                                </StackPanel>
                            </ToolTipService.ToolTip>
                            <TextBlock Text="Сопротивление"/>
                            <TextBlock Text="{Binding Druid.Resist.Value}"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Бонус к сопротивлению"/>
                            <TextBlock Text="{Binding Druid.SpellPower.BonusBase.Value}"/>
                        </StackPanel>
                        <!--Лечение (class ...)-->
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Лечение"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Уклонение"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Устойчивость к критическому удару"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Сопротивление критическому урону"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Блокирование"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Блокируемый урон"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Сила ярости"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Устойчивость к силе ярости"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Доп. урон PvE"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Доп. защита PvE"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Доп. урон PvP"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Доп. защита PvP"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Сила Атаки"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Усиление физических эффектов"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Устойчивость к физ. урону"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Усиление магических эффектов"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Устойчивость к маг. урону"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Усиление эффектов лечения"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Урон боссам"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" Spacing="30">
                            <TextBlock Text="Сопротивление обходу защиты"/>
                            <TextBlock Text="0"/>
                        </StackPanel>
                    </ListView>
                </Grid>

                <!--Окно Брони(гринда)-->
                <Grid x:Name="armorGrid" Visibility="Collapsed">
                    <Grid.Background>
                        <ImageBrush ImageSource="Images/Armor.jpg" Stretch="UniformToFill" Opacity="0.5"/>
                    </Grid.Background>
                    <Grid.RowDefinitions>
                        <RowDefinition/>
                        <RowDefinition/>
                        <RowDefinition/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                </Grid>

                <!--Окно Вех(доп бонусы)-->
                <Grid x:Name="vehaGrid" Visibility="Collapsed">
                    <Grid.Background>
                        <ImageBrush ImageSource="Images/Veha.jpg" Stretch="UniformToFill" Opacity="0.5"/>
                    </Grid.Background>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="10*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="5*"/>
                        <ColumnDefinition Width="5*"/>
                        <ColumnDefinition />
                    </Grid.ColumnDefinitions>

                    <StackPanel Grid.Row="1" Grid.Column="0" Margin="20,20,0,0" Orientation="Vertical" Spacing="5">
                        <CheckBox Content="Веха Императора" IsChecked="{Binding Druid.Veha.Imperial, Mode=TwoWay}">
                            <ToolTipService.ToolTip>
                                Сила атаки и сила заклинаний +4%. Очков характеристик + 48
                            </ToolTipService.ToolTip>
                        </CheckBox>

                        <CheckBox Content="Веха Оро" IsChecked="{Binding Druid.Veha.Oro, Mode=TwoWay}">
                            <ToolTipService.ToolTip>
                                Защита и сопротивление +4%. Очки характеристик +64
                            </ToolTipService.ToolTip>
                        </CheckBox>

                        <CheckBox Content="Веха Акари" IsChecked="{Binding Druid.Veha.Akari, Mode=TwoWay}">
                            <ToolTipService.ToolTip>
                                Максимальный запас здоровья +4%. Очки характеристик +80
                            </ToolTipService.ToolTip>
                        </CheckBox>

                        <CheckBox Content="Веха Куроми" IsChecked="{Binding Druid.Veha.Kuromi, Mode=TwoWay}">
                            <ToolTipService.ToolTip>
                                Максимальный запас здоровья +7%. Очки характеристик +128
                            </ToolTipService.ToolTip>
                        </CheckBox>

                        <CheckBox Content="Веха Рена" IsChecked="{Binding Druid.Veha.Ren, Mode=TwoWay}">
                            <ToolTipService.ToolTip>
                                Максимальный запас здоровья +10%. Очки характеристик +192
                            </ToolTipService.ToolTip>
                        </CheckBox>

                        <CheckBox Content="Веха Атума" IsChecked="{Binding Druid.Veha.Atum, Mode=TwoWay}">
                            <ToolTipService.ToolTip>
                                Максимальный запас здоровья +7%. Максимальная сила атаки и сила заклинаний +7%. Очки характеристик + 256
                            </ToolTipService.ToolTip>
                        </CheckBox>
                    </StackPanel>
                </Grid>

                <!--Окно Брактеата. Взаимодействие будет выполнено рисунками. (Доп бонусы от камней)-->
                <Canvas x:Name="stoneGrid" Visibility="Collapsed" Background="#FF24211B">
                    <Rectangle Canvas.Top="10" Canvas.Left="10" Canvas.ZIndex="0" Height="480" Width=" 530">
                        <Rectangle.Fill>
                            <ImageBrush ImageSource="Images/StonePanel.png"/>
                        </Rectangle.Fill>
                    </Rectangle>
                    <!--НЕБЕСНЫЙ КРУГ-->
                    <!--11 часов-->
                    <ComboBox Canvas.Left="189" Canvas.Top="42" Height="52" Width="52" MaxDropDownHeight="156" Padding="0">
                        <!--Творения-->
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="I" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="II" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="III" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="IV" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="V" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="VI" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="VII" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
                                    <ImageBrush ImageSource="Images/Creating.png" Stretch="None"/>
                                </Grid.Background>
                                <TextBlock Text="VIII" FontSize="20" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                            </Grid>
                        </ComboBoxItem>
                        <ComboBoxItem Height="52" Margin="0,0,32,0">
                            <Grid Margin="0,-26,-32,-26">
                                <Grid.Background>
